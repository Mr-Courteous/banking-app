{
    "version": 2,
    "builds": [{"src": "./index.js", "use": "@vercel/node"}],
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)", 
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "http://localhost:5173" 
          }
        ]
      }
    ]
  }